{
  "datasets": {
    "WikiPassageQA": {
      "evaluation": {
        "type": "fixed-split"
      }
    },

    "QAChave": {
      "evaluation": {
        "type": "cross-validation",
        "folds": 10,
        "folds-splitter": "ShuffleSplit",
        "test_size": 0.3,
        "random_state": 0
      }
    }
  },

  "pipeline": {
    "AnswerTypeClassification": {
      "technique": {
        "name": "answer-type-simple-rule-based",
        "parameter_1": 0
      },
      "datasets_ignore": ["WikiPassageQA"]
    }
  }
}
